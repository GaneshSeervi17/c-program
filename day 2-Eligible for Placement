#include <stdio.h>

int main() {
    char name[50];
    int status;
    int marks;

    
   
    scanf("%s", name);


    scanf("%d", &status);


    scanf("%d", &marks);

    
    printf("Name of the Student:%s\n", name);

    
    if (status == 1 && marks >= 60) {
        printf("%s is Eligible for Placement\n", name);
    } else {
        printf("%s is Not Eligible for Placement\n", name);
    }

    return 0;
}
