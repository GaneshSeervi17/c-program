#include <stdio.h>

int main() {
    int n, i, num;
    float product = 1, sum = 0, mean;

    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        scanf("%d", &num);
        product *= num;
        sum += num;
    }

    mean = sum / n;

    printf("Product of the array:%.2f\n", product);
    printf("Mean of the array:%.3f\n", mean);

    return 0;
}
